  <%# CSS in assets/stylesheets/components/map %>

<%= image_tag venue[:photo], alt: "Card image cap", class: "card-img-top" %>

<div class="card-body">
  <h5><%= venue[:name] %></h5>
  <div class="card-info">
    <div>
      <p><em><%= venue[:categories][0]&.humanize %></em></p>
    </div>
    <div class="more-info">
      <p> <%= venue[:rating] %> <i class="far fa-star"></i> </p>
    </div>
  </div>
</div>
